<UserControl x:Class="AI_Agent_Graphics_Resource_Downloader.可下载项"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AI_Agent_Graphics_Resource_Downloader"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>
        <Button Name="可下载项按钮">
            <TextBlock Name="可下载项名称" FontSize="16" Grid.Column="0"></TextBlock>
        </Button>
        <Grid Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <TextBlock Name="可下载项大小" Grid.Row="0" HorizontalAlignment="Right"></TextBlock>
            <TextBlock Name="可下载项时间" Grid.Row="1" HorizontalAlignment="Right"></TextBlock>
        </Grid>
    </Grid>
</UserControl>
